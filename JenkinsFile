pipeline
{
agent any
tools{
nodejs 'node16.16.0'
}
stages
{
stage('SCM')
{
steps{
git branch: 'main', url: 'https://github.com/Kuttan007/react-todo-app.git'
}
}
stage('Installing application')
{
steps{
sh 'npm install'
}
}
stage('Building')
{
steps{
sh 'npm run build'
}
}
stage('Deploying')
{
steps{

sh '''
cp -R build/* /var/www/html/'
'''
}
}
}
}
